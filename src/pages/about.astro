      <!-- Call for Papers -->
      <section class="mb-16">
        <div class="max-w-6xl mx-auto">
          <div class="text-center mb-12">
            <h2 class="text-4xl font-bold text-purple-800 mb-4">Call for Papers</h2>
            <div class="w-24 h-1 bg-purple-600 mx-auto mb-8"></div>
          </div>

          <div class="bg-white rounded-2xl shadow-xl p-8 md:p-12 border-l-8 border-green-500">
